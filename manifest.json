{
  "manifest_version": 3,
  "name": "foresttrip",
  "description": "숲나들e",
  "version": "1.0.0",
  "action": {
    "default_icon": "logo.jpg",
    "default_popup": "popup.html",
    "default_title": "숲나들e"
  },
  "web_accessible_resources": [
    {
      "resources": ["inject_start.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*/*"],
  "permissions": [
    "activeTab",
    "scripting",
    "webNavigation",
    "tabs",
    "declarativeContent",
    "storage",
    "nativeMessaging"
  ]
}
