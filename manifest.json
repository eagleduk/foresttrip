{
  "manifest_version": 3,
  "name": "foresttrip",
  "description": "숲나들e",
  "version": "1.0.0",
  "action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "숲나들e"
  },
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["https://*/*"],
  "permissions": ["scripting", "tabs", "storage"]
}
